{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "Minimalist Vue template for Web developers",
  "scripts": {
    "build": "cross-env NODE_ENV=development gulp --development",
    "start": "cross-env NODE_ENV=development gulp watch --development",
    "production": "cross-env NODE_ENV=production gulp --production"
  },
  "author": "Amin NAIRI",
  "license": "MIT",
  "devDependencies": {
    "aliasify": "^2.1.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "coffee-script": "^1.12.7",
    "coffeeify": "^3.0.0",
    "coffeescript": "^2.0.1",
    "cross-env": "^5.0.5",
    "envify": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.9.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.4.0",
    "gulp-jsonminify": "^1.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-pug": "^3.3.0",
    "gulp-remove-empty-lines": "^0.1.0",
    "gulp-remove-html": "^1.3.0",
    "gulp-sass": "^3.1.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^3.0.0",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.4",
    "rimraf": "^2.6.2",
    "uglifyify": "^4.0.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vue": "^2.4.4",
    "vue-router": "^2.7.0",
    "vueify": "^9.4.1",
    "vuex": "^3.0.0"
  },
  "browserify": {
    "transform": [
      "envify",
      "coffeeify",
      "vueify",
      "aliasify",
      "uglifyify"
    ]
  },
  "aliasify": {
    "aliases": {
      "vue": "vue/dist/vue.runtime.min.js",
      "vue-router": "vue-router/dist/vue-router.min.js"
    }
  }
}
